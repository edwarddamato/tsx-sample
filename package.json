    "style-loader": "^0.18.2",
    "ts-jest": "^21.0.1",
    "typescript": "^2",
    "typescript-eslint-parser": "^8.0.0",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.4.5"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
